<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
<ui:with field="i18n" type="org.ednovo.gooru.application.shared.i18n.MessageProperties" />
	<g:HTMLPanel>
	<div id="addCollectionPopup">
		<div class="popHeader">
			<g:Label styleName="col-md-10 col-xs-10 popHeading" ui:field="headerLbl"/>
				<div class="col-md-2"><g:Label ui:field="cancelResourcePopupBtnLbl" styleName="closeButton"></g:Label></div>
			<div class="clearfix"></div>
		</div>
	    <g:HTMLPanel styleName="popBody">
	    	<g:Label styleName="lblSuccessImage"></g:Label>
	    	<g:Label ui:field="lblSuccessText" styleName="lblSuccessText"></g:Label>
	    </g:HTMLPanel>
		<div class="clearfix"></div>
		<div class="popFooter">
			<div class="addButton leftAlign"><g:Button styleName="secondary" ui:field="btnContinueSearching" text="{i18n.GL3191}"/></div>
			<div class="addButton rightAlign"><g:Button styleName="primary" ui:field="btnViewInMyCollections" text="{i18n.GL1630_1}" title="{i18n.GL1630}"/></div>
		</div>
		<div class="clearfix"></div>
	</div>
</g:HTMLPanel>
</ui:UiBinder>