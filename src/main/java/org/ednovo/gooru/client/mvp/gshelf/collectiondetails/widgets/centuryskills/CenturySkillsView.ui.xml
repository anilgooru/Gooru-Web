<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:a="urn:import:aj.org.objectweb.asm"
	xmlns:gt="urn:import:com.tractionsoftware.gwt.user.client.ui" xmlns:uc="urn:import:org.ednovo.gooru.client.uc">
	<ui:with
		type="org.ednovo.gooru.client.mvp.shelf.collection.CollectionCBundle"
		field="res" />
		<ui:with field="i18n"
		type="org.ednovo.gooru.application.shared.i18n.MessageProperties" />
	<!-- Centure Skill -->

	<g:HTMLPanel styleName="21Century openHideBlocks">
		<uc:H4Panel>
			<g:InlineLabel text="{i18n.GL3199}"></g:InlineLabel>
		</uc:H4Panel>
		<uc:PPanel>
			<g:InlineLabel text="{i18n.GL3390}"></g:InlineLabel>
		</uc:PPanel>
		<g:FlowPanel styleName="{res.css.secondaryInfoContainer} {res.css.centuryContainer} {res.css.rightCourseContainer}">
			<g:FlowPanel ui:field="centContainer">
				<g:FlowPanel styleName="{res.css.shelfCourseSubject}">
					<g:FlowPanel styleName="{res.css.shelfNameCourse}">
						<g:FlowPanel styleName="{res.css.standardsCont}">
							<g:Button ui:field="centbrowseBtn" styleName="secondary">Browse</g:Button>
							<uc:AppCenturyTagSuggestBox ui:field="centurySgstBox" styleName="inlineBlock" width="176px" height="30px" />
							<g:Label styleName="{res.css.standardMaxHide}" ui:field="centMaxMsg" />
							<g:FlowPanel ui:field="centPanel"
								styleName="{res.css.collectionStandardsMargin}" />
						</g:FlowPanel>
					</g:FlowPanel>
				</g:FlowPanel>
			</g:FlowPanel>
			<g:HTMLPanel styleName="clearfix"></g:HTMLPanel>
		</g:FlowPanel>
		<g:HTMLPanel styleName="clearfix"></g:HTMLPanel>
	</g:HTMLPanel>
	<!-- end -->
</ui:UiBinder>