<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:sf="urn:import:org.ednovo.gooru.client.mvp.home">
	<ui:with type="org.ednovo.gooru.application.client.home.HomeCBundle"
		field="res" />
	<g:HTMLPanel>
		<div class="{res.css.featuredRightContainer} {res.css.featuredContainer}">
		<g:Hidden ui:field="collectionGooruOid"></g:Hidden>
			<g:FocusPanel ui:field="collectionStudyButtonFocPanel">
			<sf:StudyFeaturedCollection ui:field="studyFeaturedCollection"/>
			</g:FocusPanel>
			<div
				class="{res.css.featuredLeftContainerContent} {res.css.featuredContainer}">
				<div class="{res.css.featuredThemeContentContainer}">
					<div class="{res.css.featuredThemeContent}">
						<h2><g:Label ui:field="didYouKnowText"></g:Label></h2>
						<p ui:field="contentDisplayTitle"></p>
					</div>
				</div>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>