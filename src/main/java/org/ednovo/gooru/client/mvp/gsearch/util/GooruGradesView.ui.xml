	xmlns:uc="urn:import:org.ednovo.gooru.client.uc">
	<ui:with field="i18n" type="org.ednovo.gooru.application.shared.i18n.MessageProperties" />
		<g:HTMLPanel>
			<g:Label styleName="gradeHeader" text="{i18n.GL3232}" title="{i18n.GL3232}" ui:field="gradeHeader"/>

			<g:HTMLPanel styleName="gradesContainer" ui:field="gradeContainer">
			    <uc:UlPanel>
			        <uc:LiPanel styleName="gradeTypes" ui:field="preKLiPnl"><g:Anchor text="{i18n.GL3070}" title="{i18n.GL3070}"/></uc:LiPanel>
			    </uc:UlPanel>
				<uc:UlPanel ui:field="elementryPanel"></uc:UlPanel>
				<uc:UlPanel ui:field="middlePanel"></uc:UlPanel>
				<uc:UlPanel ui:field="higherPanel"></uc:UlPanel>
				<uc:UlPanel>
			        <uc:LiPanel styleName="gradeTypes" ui:field="higherLiPnl"><g:Anchor text="{i18n.GL3084}" title="{i18n.GL3084}"/></uc:LiPanel>
			    </uc:UlPanel>
			</g:HTMLPanel>
		</g:HTMLPanel>
</ui:UiBinder>