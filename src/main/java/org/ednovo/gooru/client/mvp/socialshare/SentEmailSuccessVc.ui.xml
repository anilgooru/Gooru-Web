<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gg="urn:import:org.ednovo.gooru.client.uc">
	<ui:style field="SocialShare" src="SocialShareStyles.css" />

	<g:HTMLPanel styleName="PopupMainVSmall" ui:field="popupContainer">
		<g:HTMLPanel styleName="popupInnerGrey">
			<g:HTMLPanel styleName="popupgreyHeader">
			    <g:HTMLPanel styleName="row">
			        <g:HTMLPanel styleName="col-md-12 col-xs-12" ui:field="emailToFriendText"></g:HTMLPanel>
			     </g:HTMLPanel>
			     <g:HTMLPanel styleName="clearfix"/>
			</g:HTMLPanel>
			<g:HTMLPanel>
				<g:HTMLPanel styleName="{SocialShare.classPageEmailBlueText}" ui:field="emailSentText"></g:HTMLPanel>
				<g:Label styleName="{SocialShare.classPageEmailDescText}" ui:field="toEmailLbl"></g:Label>
				<g:HTMLPanel styleName="formViewButtonContainer">
					<g:Button styleName="primary" ui:field="okLbl"/>
				</g:HTMLPanel>
			</g:HTMLPanel>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>