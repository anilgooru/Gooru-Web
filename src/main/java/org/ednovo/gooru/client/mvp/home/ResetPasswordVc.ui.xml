<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gg="urn:import:org.ednovo.gooru.client.uc">
	<g:FlowPanel styleName="resetPwdContentDiv">
		<g:Label styleName="resetPwdTextContainer" ui:field="newPasswordText"/>
		<g:FlowPanel styleName="resetPwdPanel">
			<g:Label styleName="resetPwdLabel" ui:field="newPwdLbl"/>
			<g:TextBox styleName="form-control form_control" ui:field="resetNewPwdTxtBox"/>
			<gg:ErrorLabelUc ui:field="newPwdValidationUc"/>
		</g:FlowPanel>
		<g:FlowPanel styleName="resetPwdPanel">
			<g:Label styleName="resetPwdLabel" ui:field="confirmPwdLbl"/>
			<g:TextBox styleName="form-control form_control" ui:field="resetConfirmPwdTxtBox"/>
			<gg:ErrorLabelUc ui:field="confirmPwdValidationUc"/>
		</g:FlowPanel>
		<g:FlowPanel styleName="errorMessageContainer">
			<g:Label styleName="errorMessageStyle" ui:field="tokenExpireErrorLabel"></g:Label>
		</g:FlowPanel>
		<g:FlowPanel styleName="forgotPwdFormButtonDiv">
			<gg:BlueButtonUc ui:field="sendMailBtnUc" />
			<g:Anchor ui:field="resetPwdCancelAnr" styleName="forgotPwdFormCancelButton" />
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder>