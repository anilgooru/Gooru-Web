<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="i18n" type="org.ednovo.gooru.application.shared.i18n.MessageProperties"/>

	<g:HTMLPanel styleName="summaryWrapper">
		<g:HTMLPanel styleName="summaryPopUpHeader no-gutter">
			<g:HTMLPanel styleName="col-md-7 heading">
			     <g:Label  ui:field="titleLbl" styleName="summaryTitle">Response</g:Label>
			  </g:HTMLPanel>
			<g:HTMLPanel styleName="col-md-5">
			    <g:Label  ui:field="closeLbl" styleName="summaryClose"></g:Label>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="clearfix"></g:HTMLPanel>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="summaryPopupBody">
		<g:FlowPanel ui:field="ansFlowPnl"></g:FlowPanel>
		</g:HTMLPanel>
	</g:HTMLPanel>
	
	
</ui:UiBinder>