<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
<ui:with type="org.ednovo.gooru.application.client.home.HomeCBundle" field="res" />
	<ui:style field="landingPageStyle" src="landing-page.css" />

	<g:HTMLPanel styleName="{landingPageStyle.pageSection}">
			<g:Anchor ui:field="backto" href="#" styleName="{landingPageStyle.returnToTop}"></g:Anchor>
			<g:HTMLPanel styleName="{landingPageStyle.supportersContentBlock} {landingPageStyle.contentBlock}">
			    <g:HTMLPanel styleName="{landingPageStyle.aboutBlurb}">
			        <g:HTMLPanel styleName="{landingPageStyle.aboutGooruLogo}"></g:HTMLPanel>
			        <g:Label ui:field="nonProfitLbl"></g:Label>
			        <g:Anchor ui:field="about" target="_blank"></g:Anchor><g:Label ui:field="sectionText"></g:Label>
			    </g:HTMLPanel>
				<g:Label styleName="{landingPageStyle.supportersText}" ui:field="lblWonderfulSupporters" />
				<a href="http://csr.cisco.com/pages/cisco-foundation" target="_blank">
					<g:Image styleName="{landingPageStyle.supportersImg}" ui:field="ciscoImg"/>
				</a>
				<a href="http://www.gatesfoundation.org/" target="_blank">
					<g:Image styleName="{landingPageStyle.supportersImg}" ui:field="gatesImg"/>
				</a>
				<a href="http://www.onr.navy.mil" target="_blank">
					<g:Image styleName="{landingPageStyle.supportersImg}" ui:field="onrImg"/>
				</a>
				<a href="http://www.google.org" target="_blank">
					<g:Image styleName="{landingPageStyle.supportersImg}" ui:field="googleImg"/>
				</a>
				<a href="http://nextgenlearning.org/" target="_blank">
					<g:Image styleName="{landingPageStyle.supportersImg}" ui:field="nglcImg"/>
				</a>
				<a href="http://www.pearson.com/" target="_blank">
					 <g:Image styleName="{landingPageStyle.supportersImg}" ui:field="pearsonImg"/>
				</a>
				<a href=" http://www.hewlett.org/" target="_blank">
					<g:Image styleName="{landingPageStyle.supportersImg}" ui:field="hewlettImg"/>
				</a>
				<a href="http://www.scefdn.org/" target="_blank">
					<g:Image styleName="{landingPageStyle.supportersImg}" ui:field="scefdnImg"/>
				</a>
				<a href="http://www.fenwick.com/services/practices/Pages/technology-transactions.aspx" target="_blank">
					<g:Image styleName="{landingPageStyle.supportersImg}" ui:field="fenwickImg"/>
				</a>
				<g:Image styleName="{landingPageStyle.supportersImg}" ui:field="ramImg"/>
			</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>