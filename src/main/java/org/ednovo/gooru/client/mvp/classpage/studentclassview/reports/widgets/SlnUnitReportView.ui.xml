<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:span="urn:import:org.ednovo.gooru.client.uc"
	xmlns:uc="urn:import:org.ednovo.gooru.client.ui">
    <ui:with field="i18n" type="org.ednovo.gooru.application.shared.i18n.MessageProperties" />
    <ui:style src="" />
	<g:HTMLPanel styleName="rowContainer no-gutter">
	   <uc:HTMLEventPanel styleName="col-md-3 col-sm-3" ui:field="unitBlock">
	      <g:Label styleName="numaricOrder" ui:field="numericOrder"></g:Label>
		   <g:HTMLPanel styleName="heighlightBlockUnitReports">
		      <g:HTMLPanel styleName="lessonDetails col-md-9 col-sm-9 col-xs-9">
	             <span:H3Panel ui:field="lessonCountName"></span:H3Panel>
	             <span:PPanel ui:field="lessonName"></span:PPanel>
		      </g:HTMLPanel>
		      <div class="progressBlockLeft col-md-3 col-sm-3 col-xs-3 line-height-70">
	            <g:HTMLPanel ui:field="circleIcon">
	                <span></span>
	            </g:HTMLPanel>
	         </div>
		   </g:HTMLPanel>
	   </uc:HTMLEventPanel>
	   <div class="col-md-8 col-sm-8 col-xs-8 statusScrollContainer">
	      <div class="statusScrollscrollInnerBlock no-gutter">
	         <uc:HTMLEventPanel styleName="scrollLeftArrow cursorArrows cursorPointer" ui:field="leftArrow"></uc:HTMLEventPanel>
	         <g:HTMLPanel ui:field="slnContentPanel"></g:HTMLPanel>
	         <div class="clearfix"></div>
	         <uc:HTMLEventPanel styleName="scrollRightArrow cursorArrows cursorPointer" ui:field="rightArrow"></uc:HTMLEventPanel>
	      </div>
	   </div>
	   <div class="col-md-1 col-sm-1 col-xs-12">
	      <div class="timeAndScore">
	         <g:HTMLPanel styleName="avgScore" ui:field="avgScorePanel">
	            <span:PPanel text="{i18n.GL3469_17}"></span:PPanel>
	            <span:H2Panel ui:field="totalScore"></span:H2Panel>
	         </g:HTMLPanel>
	      </div>
	   </div>
	   <div class="clearfix"></div>
	</g:HTMLPanel>
</ui:UiBinder> 