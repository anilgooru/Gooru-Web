<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gg="urn:import:org.ednovo.gooru.client.uc">
	<ui:with type="org.ednovo.gooru.application.client.home.HomeCBundle" field="res" />
	<g:HTMLPanel>
		<g:FlowPanel styleName="{res.css.resetPwdSuccessContainer}"
			height="100px">
			<g:FlowPanel styleName="{res.css.resetTimeoutHeader}">
				<g:Label ui:field="newPwdLbl"/>
			</g:FlowPanel>

			<g:FlowPanel styleName="{res.css.resetTimeoutText}">
				<g:Label ui:field="timeOutText"/>
			</g:FlowPanel>

			<g:FlowPanel styleName="{res.css.resetFooterText}">
				<g:Label ui:field="requestNewText" styleName="{res.css.linkTxt}"/>
					<g:Label styleName="{res.css.forgotPwdLink}" ui:field="forgotPwdLbl"/>
			</g:FlowPanel>
		</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder>