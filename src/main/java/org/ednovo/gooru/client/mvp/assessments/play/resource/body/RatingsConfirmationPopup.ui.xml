<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:htmlevents="urn:import:org.ednovo.gooru.client.ui">

	<g:HTMLPanel ui:field="ratingsMainContainer">
	    <g:HTMLPanel styleName="ratingConfirmationContainer">
        	<g:HTMLPanel styleName="arrowBorder"></g:HTMLPanel>
            <g:HTMLPanel styleName="arrow"></g:HTMLPanel>
            <g:HTMLPanel styleName="icon complete" ui:field="imgSuccessIcon"></g:HTMLPanel>
            <g:Label styleName="ratingText" ui:field="reviewRatingText"></g:Label>
               <!--  <h3>Thank you for reviewing this resource!</h3> -->
                  <g:HTMLPanel styleName="rating" ui:field="averageRationPanel">
                      <br/>
                    </g:HTMLPanel>
                     <g:FlowPanel ui:field="ratingWidgetPanel"></g:FlowPanel>
                    <g:HTMLPanel styleName="okContainer" ui:field="buttonsContainer">
                        <g:Button ui:field="btnOk" styleName="primary"></g:Button>
                    </g:HTMLPanel>
                    <g:Label ui:field="saveAndPsotLbl"></g:Label>
                </g:HTMLPanel>
	    
	    
	</g:HTMLPanel>
	
</ui:UiBinder>