		    <g:Label ui:field="lblGooruFieldTripDescUnforseen" styleName="h5 textColorWhite"/>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="margin40Px">
		    <g:Label ui:field="lblGooruFieldTripDescOriginal" styleName="h5 textColorWhite"/>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="margin40Px">
			<g:Button styleName="primary" ui:field="btnResourceLink" />
		</g:HTMLPanel>
		<g:HTMLPanel>
		    <g:Label ui:field="lblDontForget" styleName="h5 resourceComeBack"/>
