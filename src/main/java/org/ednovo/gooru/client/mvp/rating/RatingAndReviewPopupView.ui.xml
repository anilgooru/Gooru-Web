<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<!-- <ui:style field="style" src="ratingandreview.css"/> -->
	<g:HTMLPanel styleName="PopupMainVSmall" ui:field="maincontainer">
            <g:HTMLPanel styleName="popupInnerGrey">
                <g:HTMLPanel styleName="popupgreyHeader">
                    <g:HTMLPanel styleName="row">
                    	<g:HTML styleName="col-md-8 col-xs-8"  ui:field="lblResourceTitle"></g:HTML>
                    	<g:HTMLPanel styleName="col-md-4 col-xs-4 closeContainer">
                    		<g:Label styleName="closeButton" ui:field="closeButton"></g:Label>
                    	</g:HTMLPanel>
                    	<g:HTMLPanel styleName="clearfix"></g:HTMLPanel>
                    </g:HTMLPanel>
                 </g:HTMLPanel>
               <g:ScrollPanel ui:field="reviewScrollPanel">
                <g:HTMLPanel>
                    <g:HTMLPanel>
                        <g:HTMLPanel styleName="reviewsContent">
                            <g:HTMLPanel styleName="col-md-6 col-sm-6 ratingDistribution">
                            <g:Label styleName="rHeader" ui:field="ratingDistributionLbl"></g:Label>
                            <g:HTMLPanel ui:field="panelRatingLabels" styleName="col-md-4 col-xs-4 panelRatingLabels">
                                <g:Label styleName="ratingLabel" ui:field="excellentLbl"></g:Label>
                                <g:Label styleName="ratingLabel" ui:field="veryGoodLbl"></g:Label>
                                <g:Label styleName="ratingLabel" ui:field="goodLbl"></g:Label>
                                <g:Label styleName="ratingLabel" ui:field="fairLbl"></g:Label>
                                <g:Label styleName="ratingLabel" ui:field="poorLbl"></g:Label>
                             </g:HTMLPanel>
                             <g:HTMLPanel ui:field="panelRatingValues" styleName="col-md-8 col-xs-8  panelRatingValues">
                                	<g:HTMLPanel styleName="ratingLabel" >
                                	   <g:HTMLPanel styleName="bar" ui:field="dataFive"></g:HTMLPanel>
                                	   <g:Label ui:field="excellentScore" styleName="count"></g:Label>
                                	</g:HTMLPanel>
									<g:HTMLPanel styleName="ratingLabel" >
										<g:HTMLPanel styleName="bar data-two" ui:field="dataFour"></g:HTMLPanel>
                                		<g:Label ui:field="verygoodScore" styleName="count"></g:Label>
									</g:HTMLPanel>
                                   	<g:HTMLPanel styleName="ratingLabel" >
                                   		<g:HTMLPanel styleName="bar data-three" ui:field="dataThree"></g:HTMLPanel>
                                   		<g:Label ui:field="goodScore" styleName="count"></g:Label>
                                   	</g:HTMLPanel>
                                   	<g:HTMLPanel styleName="ratingLabel" >
                                   		<g:HTMLPanel styleName="bar data-four" ui:field="dataTwo"></g:HTMLPanel>
                                   		<g:Label ui:field="fairScore" styleName="count"></g:Label>
                                   	</g:HTMLPanel>
                                   	<g:HTMLPanel styleName="ratingLabel" >
                                   		<g:HTMLPanel styleName="bar data-five" ui:field="dataOne"></g:HTMLPanel>
                                   		<g:Label ui:field="poorScore" styleName="count"></g:Label>
                                   	</g:HTMLPanel>

                             </g:HTMLPanel>
                             <g:HTMLPanel styleName="clearfix"> </g:HTMLPanel>
                        </g:HTMLPanel>
                        <g:HTMLPanel styleName="col-md-6 col-sm-6 AvgRate">
                       		<g:Label styleName="rHeader" ui:field="avgLbl"></g:Label>
                         	<g:FlowPanel ui:field="ratingWidgetPanel" styleName="ratingStars"></g:FlowPanel>
                         	<g:Button styleName="secondary" ui:field="rateResourceBtn"></g:Button>
                        </g:HTMLPanel>
                        <g:HTMLPanel styleName="clearfix"></g:HTMLPanel>
                       	</g:HTMLPanel>
						<g:HTMLPanel styleName="userRating" ui:field="userRatingContainer">
					   		<g:Label styleName="rateMsg" ui:field="rateMsg"></g:Label>
					  		 <!-- <g:Button styleName="secondary {style.floatRight}" ui:field="rateResourceBtn"></g:Button> -->
						</g:HTMLPanel>
                    </g:HTMLPanel>
                    <g:VerticalPanel ui:field="reviewsContainer">
                    </g:VerticalPanel>
                </g:HTMLPanel>
              </g:ScrollPanel>
            </g:HTMLPanel>
        </g:HTMLPanel>
</ui:UiBinder>