<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
<ui:with type="org.ednovo.gooru.client.mvp.authentication.SignUpCBundle"
		field="res" />
	<g:HTMLPanel styleName="{res.css.popup} {res.css.imgBG}">
		<g:HTMLPanel styleName="{res.css.popupInnerLeaveReg}">
			<g:Label styleName="{res.css.popupHeader}" ui:field="lblTitle" />
			<g:Label styleName="{res.css.closeButton}" ui:field="lblCancel"></g:Label>
			<g:HTMLPanel ui:field="panelSignUp">
				<g:HTMLPanel styleName="{res.css.popupContentLeaveRegistration}">
					<g:Label styleName="{res.css.h1} {res.css.green}"  ui:field="lblHeading" />
					<g:Label styleName="{res.css.h2DontWHeader}" ui:field="lblSubHeading" />
				</g:HTMLPanel>
				</g:HTMLPanel>

			<g:HTMLPanel styleName="{res.css.leaveRegistrationButtonContainer}">
				<g:Button ui:field="btnGoToSetting" styleName="secondary"></g:Button>
				<g:Button ui:field="btnOk" styleName="primary"></g:Button>
			</g:HTMLPanel>
				</g:HTMLPanel>

		<g:HTMLPanel styleName="{res.css.leaveRegistrationBottomContainer}">
			    <g:HTMLPanel styleName="{res.css.leaveRegistarionFooterText}">
			    <g:Label ui:field="quriesText"/>
			    <g:InlineLabel  ui:field="pleaseContactText"/> <g:Anchor  ui:field="supportLink" target="_blank"></g:Anchor>
			    </g:HTMLPanel>
			</g:HTMLPanel>
				</g:HTMLPanel>
</ui:UiBinder>