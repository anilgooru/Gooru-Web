<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
xmlns:gg="urn:import:org.ednovo.gooru.client.uc">
<ui:with type="org.ednovo.gooru.application.client.home.HomeCBundle" field="res" />
    <g:HTMLPanel>
        <g:Label styleName="{res.css.loginTop}" ui:field="loginLbl">
        </g:Label>
        <div aligh="left" class="{res.css.loginBorderPanel}" ui:field="loginBorderPanel">
            <div class="{res.css.loginGooruLogoContainer}">
                <div class="{res.css.loginGooruLabel}"></div>
                <g:Label styleName="{res.css.loginGooruLabelSubText}" ui:field="betaText"></g:Label>
            </div>
            <g:Label ui:field="loginToCreateteLbl" styleName="{res.css.loginBlackText}"/>
            <div align="left" class="{res.css.textBoxesField}">
                <gg:TextBoxWithPlaceholder ui:field="loginTxtBox"/>
                <gg:TextBoxWithPlaceholder ui:field="passwordTxtBox"  password="true"/>
                <g:Label styleName="{res.css.loginHyplink}" ui:field="forgotPwd"/>
                <div align="center" class="{res.css.loginCkbox}">
                    <g:CheckBox ui:field="keepMeLoginLbl"></g:CheckBox>
                </div>
            </div>
            <div class="{res.css.loginCancelButton}">
                <g:Button ui:field="loginButton" styleName="{res.css.loginBtn}"/>
                <g:Anchor ui:field="cancelButton" styleName="{res.css.cancelBtn}"/>
            </div>
            <div class="{res.css.register}">
                <g:Label ui:field="doNotHaveAcLbl" styleName="{res.css.registerLinkQ}" />
                <g:Label styleName="{res.css.registerLink}" ui:field="registerHereLbl"/>
            </div>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>