<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:uc="urn:import:org.ednovo.gooru.client.uc"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src=""></ui:style>
	      <g:HTMLPanel styleName="myClasesGettingStartMainBlock whiteBG PaddingGutter">
	         <div class="myClassesCodeSearch no-gutter">
	            <div class="col-md-6 col-sm-6">
	               <uc:H3Panel ui:field="studentHeader"></uc:H3Panel>
	            </div>
	            <div class="col-md-6 col-sm-6 classcodeBox">
	                <uc:TextBoxWithPlaceholder styleName="form-control" ui:field="txtClassCode"/>
		            <g:Button styleName="primary" ui:field="codeBtnEnter"></g:Button>
	            </div>
	            <div class="clearfix"></div>
	         </div>
	         <div class="myClassesSteps no-gutter">
	            <uc:H2Panel ui:field="teacherHeader"></uc:H2Panel>
	            <div class="col-md-6 col-sm-6">
	               <div class="stepsBlock">
	                  <div class="stepsBlockInner">
	                     <g:InlineLabel styleName="numaricCircle" ui:field="noteOne">
	                     </g:InlineLabel>
	                     <div class="StepsContent">
	                        <uc:H3Panel ui:field="createPanel"></uc:H3Panel>
	                        <uc:PPanel ui:field="hintPanel"></uc:PPanel>
	                     </div>
	                  </div>
	                  <div class="emptyCreateClass">
	                      <g:Button styleName="primary" ui:field="btnCreateClass"></g:Button>
	                  </div>
	                  <div class="stepsBlockInner">
	                     <g:InlineLabel styleName="numaricCircle" ui:field="noteTwo">
	                     </g:InlineLabel>
	                     <div class="StepsContent">
	                        <uc:H3Panel ui:field="courseContentPanel"></uc:H3Panel>
	                        <uc:PPanel ui:field="courseNotesPanel"/><g:Anchor ui:field="courseAnr"></g:Anchor>
	                     </div>
	                  </div>
	                  <div class="stepsBlockInner">
	                     <g:InlineLabel styleName="numaricCircle" ui:field="noteThree">
	                     </g:InlineLabel>
	                     <div class="StepsContent">
	                        <uc:H3Panel ui:field="classCodePanel"></uc:H3Panel>
	                     </div>
	                  </div>
	               </div>
	            </div>
	            <div class="col-md-6 col-sm-6">
	               <img src="images/g-desktop.png" class="img-responsive"/>
	            </div>
	            <div class="clearfix"></div>
	            <div class="supportLink">
	               <p><g:Anchor ui:field="moreLinkAnr"></g:Anchor></p>
	            </div>
	         </div>
	      </g:HTMLPanel>
</ui:UiBinder> 
