<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with type="org.ednovo.gooru.client.mvp.authentication.SignUpCBundle"
		field="res" />
		<ui:style>
		.marginTop{
			margin-top: 50px;
		}
		</ui:style>
	<g:HTMLPanel styleName="LoginpopupMain">
		<g:HTMLPanel styleName="LoginpopupInner"  ui:field="panelPopupInner">
			<g:HTMLPanel styleName="LoginpopupHeader">
				<g:HTMLPanel styleName="row">
					<g:HTMLPanel styleName="col-md-8 col-xs-8">
						<g:Label  ui:field="lblLoginHeading"></g:Label>
					</g:HTMLPanel>
					<g:HTMLPanel styleName="col-md-4 col-xs-4 closeContainer">
						<g:Anchor styleName="closeButton" ui:field="lblClose"></g:Anchor>
					</g:HTMLPanel>
					<g:HTMLPanel styleName="clearfix"></g:HTMLPanel>
				</g:HTMLPanel>
			   </g:HTMLPanel>
			<g:HTMLPanel styleName="col-md-12 col-xs-12 text-center {style.marginTop}">
				<g:Label styleName="{res.css.congratsHeader}" ui:field="lblCongratsHeader"/>
				<g:Label styleName="subheader" ui:field="lblCheckYourEmail"/>
				<g:Button styleName="primary {res.css.startUsingGooru}" ui:field="btnStartUsingGooru"/>
			</g:HTMLPanel>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>