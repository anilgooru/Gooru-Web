<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gg="urn:import:org.ednovo.gooru.client.uc">
	<ui:with type="org.ednovo.gooru.application.client.home.HomeCBundle"
		field="res" />
	<g:HTMLPanel>
		<g:FlowPanel styleName="{res.css.resetPwdSuccessContainer}" height="100px">
			<g:FlowPanel styleName="{res.css.resetSuccessText}">
				<g:Label ui:field="resetPasswordLbl" styleName="{res.css.resetPwdSuccessText}" />
			</g:FlowPanel>
			<g:FlowPanel styleName="{res.css.okBlueBtn}" ui:field="buttonContainer">
				<gg:BlueButtonUc ui:field="okBtnUc" styleName="overRideBlueButton"/>
			</g:FlowPanel>
		</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder>