<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with type="org.ednovo.gooru.application.client.home.HomeCBundle"
		field="res" />
	<g:HTMLPanel>
		<g:FlowPanel
			styleName="{res.css.featuredRightContainerContent} {res.css.featuredContainer}">
			<g:FlowPanel styleName="{res.css.featuredContentImageContainer}"
				ui:field="contentImageFloPanel">
				<g:Image width="280px" height="215px" ui:field="contentUrlImg"/>
			</g:FlowPanel>
		</g:FlowPanel>
		<g:FlowPanel>
			<g:FlowPanel ui:field="featuredStartStudyFloPanel"
				styleName="{res.css.homePageFeaturedStartStudy}">
				<g:Label styleName="{res.css.studyFeaturedCollectionButton}"
					ui:field="studyFeaturedCollectionButton"></g:Label>
				<g:FlowPanel styleName="{res.css.studyFeaturedCollectionArrow}"></g:FlowPanel>
			</g:FlowPanel>
		</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder>