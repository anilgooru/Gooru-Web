<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:span="urn:import:org.ednovo.gooru.client.uc">
	
    <ui:with field="i18n" type="org.ednovo.gooru.application.shared.i18n.MessageProperties" />
    
	<ui:style src=""/>
	<g:HTMLPanel styleName="popupModalContent modal-widget-1000" ui:field="contentPanel">
		<g:HTMLPanel styleName="popupHeader">
		    <g:HTMLPanel styleName="row">
		        <g:HTMLPanel styleName="col-md-8 col-xs-8">
		            <g:Label ui:field="popupTitle"></g:Label>
				</g:HTMLPanel>
		        <g:HTMLPanel styleName="col-md-4 col-xs-4 closeContainer">
		            <g:Anchor styleName="closeButton" ui:field="closeButton"></g:Anchor>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="clearfix"></g:HTMLPanel>
			</g:HTMLPanel>
		</g:HTMLPanel>
		<g:HTMLPanel ui:field="popupContent" styleName="student-view-popup-modal">
			<div class="learningHeaderBlock margin-top-zero">
				<div class="col-md-6 col-sm-6">
		      		<div class="student-report-details-popup-panel"> 
		      		    <g:Image ui:field="userImage"/> 
		      		    <span:SpanPanel ui:field="userName"></span:SpanPanel>
		      		</div>
				</div>
				<div class="col-md-6 col-sm-6">
				   <g:HTMLPanel styleName="scoreStatus padding-top-10" ui:field="scoreWidget">
		               <g:Label styleName="red" text="{i18n.GL3469_19}"></g:Label>
		               <g:Label styleName="org" text="{i18n.GL3469_20}"></g:Label>
		               <g:Label styleName="yellowgreen" text="{i18n.GL3469_21}"></g:Label>
		               <g:Label styleName="lightgreen" text="{i18n.GL3469_22}"></g:Label>
		               <g:Label styleName="green" text="{i18n.GL3469_23}"></g:Label>
				   </g:HTMLPanel>
				</div>
				<div class="clearfix"></div>
			</div>
			<g:HTMLPanel ui:field="panel"></g:HTMLPanel>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>