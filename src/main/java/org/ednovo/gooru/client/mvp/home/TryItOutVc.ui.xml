<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gg="urn:import:org.ednovo.gooru.client.uc">
	<ui:with type="org.ednovo.gooru.application.client.home.HomeCBundle"
		field="res" />
	<g:HTMLPanel styleName="{res.css.tryItoutContainer}">
		<g:FlowPanel styleName="{res.css.headerPopUpTitle}">
			<g:Label styleName="{res.css.popUpTitleContext}" ui:field="improveGooruText"/>
			<g:Label styleName="{res.css.closePopUp}" ui:field="closePopUpLbl" />
		</g:FlowPanel>
		<g:FlowPanel styleName="{res.css.innerContainer}">
			<g:FlowPanel styleName="{res.css.landingPageOuterDiv}">
				<g:Label styleName="{res.css.landingPageCelebrationText}"
					ui:field="celebrationLbl"/>
				<g:Anchor styleName="{res.css.landingPageLearn}" target="_blank" ui:field="learnMoreLbl"></g:Anchor>
			</g:FlowPanel>
			<g:FlowPanel styleName="{res.css.celebrations}">
				<g:Label styleName="{res.css.landingPageCelebration}" />
			</g:FlowPanel>
		</g:FlowPanel>
		<g:FlowPanel styleName="{res.css.classicContainer}">
			<g:FlowPanel styleName="{res.css.backToClassic}">
				<g:FlowPanel styleName="{res.css.classicInnercontainer}">
					<g:Anchor styleName="{res.css.fadeblueButtonText}"
						href="" target="_blank" ui:field="backToClassicAnr" />
					<g:Anchor styleName="{res.css.fadeblueArrow}" href=""
						target="_blank" />
				</g:FlowPanel>
				<g:Label styleName="{res.css.classicGooru}"
					ui:field="classicGooruLbl"/>
			</g:FlowPanel>
			<g:FlowPanel styleName="{res.css.tryItOut}">
				<g:FlowPanel styleName="{res.css.classictryItOut}"
					ui:field="tryItOutNewGooruFloPanel">
					<g:Anchor styleName="{res.css.blueButtonText}" ui:field="tryItOutLbl"/>
					<g:Anchor styleName="{res.css.blueArrow}" />
				</g:FlowPanel>
				<g:Label styleName="{res.css.tryItOutGooru}" ui:field="goBackText"/>
			</g:FlowPanel>
		</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder>