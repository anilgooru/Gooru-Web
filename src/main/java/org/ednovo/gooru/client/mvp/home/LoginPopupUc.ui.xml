<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gg="urn:import:org.ednovo.gooru.client.uc"
	xmlns:panel="urn:import:org.ednovo.gooru.client.ui">
	<g:HTMLPanel styleName="LoginpopupMain">
		<g:HTMLPanel styleName="LoginpopupInner">
			<g:HTMLPanel styleName="LoginpopupHeader">
				<g:HTMLPanel styleName="row">
					<g:HTMLPanel styleName="col-md-8 col-xs-8">
						<g:Label  ui:field="lblLoginHeading"></g:Label>
					</g:HTMLPanel>
					<g:HTMLPanel styleName="col-md-4 col-xs-4 closeContainer">
						<g:Anchor styleName="closeButton" ui:field="cancelButton"></g:Anchor>
					</g:HTMLPanel>
					<g:HTMLPanel styleName="clearfix"></g:HTMLPanel>
				</g:HTMLPanel>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="LoginpopupContent">
				<g:Label styleName="LoginPopUpgreen" ui:field="lblWelcomeBack" />
				<g:HTMLPanel styleName="signInContainer">
					<g:HTMLPanel styleName="emailSignIn">
					    <g:Label ui:field="lblLoginWithGooru" styleName="loginWithGooru" />
						<br />
						<g:Button ui:field="gmailButton" styleName="gConnectButton"></g:Button>

						<g:HTMLPanel styleName="divider">
							<g:Label styleName="dividerText" ui:field="lblOr" />
						</g:HTMLPanel>

						<gg:TextBoxWithPlaceholder styleName="form-control form_control" ui:field="loginTxtBox" setFocus="true"/>
						<gg:TextBoxWithPlaceholder styleName="form-control form_control" ui:field="passwordTxtBox" password="true" />

						<g:HTMLPanel styleName="forgotPasswordLink">
							<g:Anchor ui:field="forgotPwd"></g:Anchor>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="loginPopupbtnContainer">
							<g:Button ui:field="loginButton" styleName="primary"></g:Button>
							<g:Label ui:field="lblPleaseWait" styleName="processing"></g:Label>
						</g:HTMLPanel>

					</g:HTMLPanel>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="haveAccountContainer">
					<g:Label styleName="haveAccount" ui:field="lblDoYouHaveAccount" />
					<g:Anchor ui:field="ancRegisterHere" />
				</g:HTMLPanel>
			</g:HTMLPanel>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>