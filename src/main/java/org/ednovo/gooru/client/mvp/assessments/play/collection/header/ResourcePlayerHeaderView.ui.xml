<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gg="urn:import:org.ednovo.gooru.client.uc">
<gg:HeaderPanel>
		<g:HTMLPanel styleName="container">
		    <g:HTMLPanel styleName="row">
		    	<g:FlowPanel styleName="col-md-6 col-xs-8 col-sm-6">
					<g:HTML styleName="playerResourceTitle"	ui:field="resourceTitle"></g:HTML>
				</g:FlowPanel>
				<g:FlowPanel styleName="col-md-6 col-xs-12 col-sm-6  rPlayerMenu">
					<g:HTMLPanel styleName="tabsection">
					      <g:Button	styleName="flagButtonOrange flagButtonNormal"
						ui:field="flagButton"></g:Button>
					<g:Button styleName="flagButtonOrange add_button_normal"
						ui:field="addButton"></g:Button>
					<g:Button styleName="flagButtonOrange info_button_normal"
						ui:field="infoButton"></g:Button>
					<g:Button	styleName="flagButtonOrange flagButtonOrange"
						ui:field="shareButton"></g:Button>
					</g:HTMLPanel>
				</g:FlowPanel>
			<g:FlowPanel styleName="resourcePlayer_close_button">
			    <g:Anchor  ui:field="closeButton"></g:Anchor>
			</g:FlowPanel>
		    </g:HTMLPanel>
		</g:HTMLPanel>
	</gg:HeaderPanel>
</ui:UiBinder>