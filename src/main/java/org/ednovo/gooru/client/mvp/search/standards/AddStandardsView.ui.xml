<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:panel="urn:import:org.ednovo.gooru.client.ui"
	xmlns:uc="urn:import:org.ednovo.gooru.client.uc">
	<ui:style field="createCollectionStyle" src="CreateCollectionPopUp.css"/>
	<g:HTMLPanel styleName="standarBodyContainer">
		<g:Button styleName="secondary" ui:field="commonStandards"></g:Button>
		<g:Button styleName="secondary" ui:field="texasKnowledge"></g:Button>
		<g:Button styleName="secondary" ui:field="ngss"></g:Button>
		<g:Button styleName="secondary"  ui:field="californiaStandards"></g:Button>
		<g:HTMLPanel styleName="clearfix"></g:HTMLPanel>
		<g:HTMLPanel styleName="listGroup">
		<g:HTMLPanel styleName="col-md-2 col-sm-2 searchStandard1">
		    <g:HTMLPanel styleName="nav-wrap scrollBar">
		        <g:Label styleName="menu-icon" ui:field="subjects">Select Subjects....</g:Label>
					<uc:UlPanel  ui:field="levelOneStandards" styleName="list-unstyled standeredList"/>
		    </g:HTMLPanel>

		</g:HTMLPanel>
		<g:HTMLPanel styleName="col-md-4 col-sm-4 searchStandard2">
		      <g:HTMLPanel styleName="nav-wrap scrollBar">
		        <g:Label styleName="menu-icon" ui:field="grade">Select Courses....</g:Label>
				<uc:UlPanel  ui:field="levelTwoStandards"  styleName="list-unstyled standeredList"/>
		    </g:HTMLPanel>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="col-md-2 col-sm-2 searchStandard3">
		    <g:HTMLPanel styleName="nav-wrap scrollBar">
		        <g:Label styleName="menu-icon" ui:field="course">Select Units....</g:Label>
				<uc:UlPanel  ui:field="levelThreeStandards" styleName="list-unstyled standeredList"/>
		    </g:HTMLPanel>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="col-md-4 col-sm-4 searchStandard4">
		    <g:HTMLPanel styleName="nav-wrap scrollBar">
		        <g:Label styleName="menu-icon menu-icon" ui:field="standard">Select Standards code....</g:Label>
				<uc:UlPanel  ui:field="levelFourStandards" styleName="list-unstyled standeredList"/>
		    </g:HTMLPanel>
		</g:HTMLPanel>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="clearfix"/>
		<uc:PPanel styleName="text-right">
		    <g:Button styleName="secondary" ui:field="addBtn">Add</g:Button>
		</uc:PPanel>

	</g:HTMLPanel>
</ui:UiBinder>