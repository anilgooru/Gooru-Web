<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel>
	<g:FocusPanel ui:field="focusPanel" styleName="Uc-profilePageEditDecsription">
		<g:DeckPanel ui:field="deckPanel">
			<g:HTMLPanel ui:field="editLabel" styleName="Uc-profilePageEditDecsription"/>
			<g:TextArea styleName="Uc-profilePageTextAreaDecsription" ui:field="editTextBox" />
		</g:DeckPanel>
	</g:FocusPanel>
	<g:Label  ui:field="errorLabelForEditText"></g:Label>
	<g:Label ui:field="errorLabel" styleName="Uc-errorValidation"></g:Label>
	</g:HTMLPanel>
</ui:UiBinder>